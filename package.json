{
  "name": "bioviz-js",
  "version": "0.1.4",
  "description": "Library to visualize biological data",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "git@gitlab.pasteur.fr:rplanel/bioviz-js.git",
  "author": "Remi Planel <rplanel@pasteur.fr>",
  "license": "MIT",
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "D3",
    "bioinformatics",
    "dataviz",
    "visualization"
  ],
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags && git push github && git push  --tags github",
    "dev": "parcel src/scripts/main-genome-browser.ts",
    "compile": "tsc",
    "build": "tsc",
    "watch-ts": "tsc -w",
    "lint": "tslint -p tsconfig.json",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.10",
    "docusaurus-init": "^1.14.1",
    "jest": "^27.0.4",
    "rxjs-tslint": "^0.1.7",
    "ts-jest": "^27.0.4",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@types/d3": "^6.2.0",
    "@types/plotly.js": "^1.54.2",
    "@types/rx": "^4.1.1",
    "@types/topojson-client": "^3.1.0",
    "colorbrewer": "^1.5.1",
    "d3-array": "^3.0.1",
    "d3-axis": "^3.0.0",
    "d3-brush": "^2.1.0",
    "d3-color": "^3.0.1",
    "d3-drag": "^2.0.0",
    "d3-geo": "^3.0.1",
    "d3-path": "^2.0.0",
    "d3-scale": "^3.2.3",
    "d3-scale-chromatic": "^3.0.0",
    "d3-selection": "^3.0.0",
    "d3-zoom": "^3.0.0",
    "parcel-bundler": "^1.12.4",
    "plotly.js-dist": "^1.57.1",
    "rxjs": "^6.5.4",
    "topojson-client": "^3.1.0",
    "tslint": "^5.20.1"
  }
